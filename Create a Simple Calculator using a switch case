#include<stdio.h>
int main()
{
char a;
int n1,n2;
printf("enter operators");
scanf("%c",&a);
printf("enter two integer");
scanf("%d %d",&n1,n2);
switch(a)
{
case '+':
printf("%d+%d=%d",n1,n2,n1+n2);
case '-':
printf("%d-%d=%d",n1,n2,n1-n2);
case'*':
printf("%d*%d=%d",n1,n2,n1*n2);
case'/':
printf("%d/%d=%d",n1,n2,n1/n2);
default:
printf("error");
}
return 0;
}
