#include<stdio.h>
#define size 3
int main()
{
int a[size][size];
int row,col,sum=0;
printf("enter element of matrix %d*%d",size,size);
for(row=0; row<SIZE; row++)
{
for(col=0; col<SIZE; col++)
{
 scanf("%d", &A[row][col]);
}
}
for(row=0; row<SIZE; row++)
{
sum = sum + A[row][row];
}
printf("\nSum of main diagonal elements = %d", sum);
return 0;
}
