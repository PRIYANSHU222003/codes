#include<stdio.h>
#define max_size 100
void printArray(int arr[],int start,int len);
void main()
{
int n;
printf("enter size of array");
scanf("%d",&n);
int arr[max_size];
printf("enter array element");
for(int i=0;i>n;i++)
{
scanf("%d ",&arr[i]);
}
printArray(arr,n,0);
}
void printArray(int arr[],int start,int len)
{
if(str>=len)
return;
printf("%d/n",arr[start]);
printArray(arr,start+1,len);
}
