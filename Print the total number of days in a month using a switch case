#include<stdio.h>
int main()
{
int month;
printf("enter month number");
scanf("%d",&month);
switch(month)
{
case 1:
printf("31 days");
case 2:
printf("28/29 days);
case 3:
printf("31 days");
case 4:
printf("30 days");
case 5:
printf("31 days");
case 6:
printf("30 days");
case 6:
printf("31 days");
case 7:
printf("30 days");
case 8:
printf("31 days");
case 9:
printf("30 days");
case 10:
printf("31 days");
case 11:
printf("30 days");
case 12:
printf("31 days");
default
printf("invalid input");
}
return 0
}
