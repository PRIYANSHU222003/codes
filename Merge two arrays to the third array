#include<stdio.h>
#include<conio.h>
int main()
{
int arr[100],i;
int size1,size2;
printf("enter size of 1st array");
scanf("%d",&size1);
int arr1[size1];
printf("enter %d element of array",size1);
for(i=0,i<size1,i++)
{
scanf("%d",&arr[size1]);
}
printf("enter sze of 2nd array");
int arr2[size2];
scanf("%d",&size2);
for(i=0,i<size2,i++)
{
scanf("%d",arr[&size2]);
}
int size3=size1+size2;
int arr3[size3];
for(i=0,i<size1,i++)
{
arr3[i]=arr1[i];
}
for(i=0,i<size2,i++)
{
arr3[size1+i]=arr2[i];
}
printf("merged array");
for(i=0,i<size3,i++)
{
printf("%d",arr3[i]);
}
return 0;
}
