#include<stdio.h>
#include<string.h>
int main()
{
int length,i;
char string1;
int flag =0;
printf("enter string");
scanf("%s",string 1);
length=strlen(string 1);
for(i=0;i<length/2;i++)
{
if(string 1[i]!=string 1[length-i-1])
{
flag=1;
break;
}
}
if(flag)
{
printf("it is not palidrome",string 1);
}
else
{
printf("it is palidrome",string 1);
}
return 0;
}
