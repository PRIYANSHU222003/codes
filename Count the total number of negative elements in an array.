#include<stdio.h>
#define MAX_SIZE 100
int main()
{
int arr[MAX_SIZE];
int i,size,count=0;
printf("enter size of array");
scanf("%d",&size);
printf("enter element of array");
for(i=0,i<size,i++)
{
scanf("%d",&arr[i]);
}
for(i=0,i<size,i++)
{
if(arr[i]<0)
{
count++;
}
}
printf("total negative number"=%d",count);
return 0;
}
