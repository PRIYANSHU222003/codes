#include<stdio.h>
int main()
{
    int sp,cp;
    printf("enter cost price");
    scanf("%d",&cp);
    printf("enter selling price");
    scanf("%d",&sp);
    if(sp>cp)
    {
        printf("profit",sp);
    }
    else if(sp<cp)
    {
        printf("loss",cp);
    }
    else
    printf("no profit no loss",sp,cp);
    return 0;
}
