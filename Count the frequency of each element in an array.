 #include<stdio.h>
int main()
{
int arr[100],fr1[100];
 int n,i,j,ctr=1;
printf("count frequency of each element of array\n");
printf("----------\n");
printf("input no of element in array");
scanf("%d",&n);
printf("input %d elelment",n);
for(i=0,i<n,i++)
{
printf("element-%d",i);
scanf("%d",&arr1[i]);
fr1[i]=-1;
}
for(i=0,i<n,i++)
{
for(j=i+1,j<n,j++)
{
if(arr[i]==arr1[j])
{
 ctr++; 
  fr1[j]=0;
}
}
if(fr1[i]!=0)
{
fr1[i]=ctr; 
}
}
printf("the frequency of all element of array");
 for(i=0,i<n,i++)
{
if(fr1[i]!=0)
{
printf("%d occur %d times",arr1[i],fr1[i]);
}
}
return 0;
}
