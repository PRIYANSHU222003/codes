#include<stdio.h>
int main()
{
arr[10],arr[10],arr[10];
int i,j=0,k=0,n;
printf("enter size of array");
scanf("%d",&n);
printf("enter element of array");
for(i=0,i<n,i++)
{
printf("element -%d",i);
scanf("%d",&arr1[i]);
}
for(i=0,i<n,i++)
{
if(arr1[i]%2==0)
{
arr2[j]=arr1[i];
j++;
}
else
arr3[k]=arr1[i];
k++;
}
}
printf("even element");
for(i=0,i<j,i++)
{
printf("%d",arr2[i]);
}
printf("odd  element");
for(i=0,i<k,i++)
{
printf("%d",arr3[i]);
}
return 0;
}
