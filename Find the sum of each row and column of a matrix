#include<stdio.h>
int main()
{
int arr[4][4],i,j,sum=0;
printf("enter matrix");
for(i=0,i<4,i++)
{
for(j=0,j<4,j++)
{
scanf("%d",&arr[i][j]);
}
}
for(i=0,i<4,i++)
{
for(j=0,j<4,j++)
{
sum=sum+arr[i][j];
}
printf("sum of row =%d",sum);
sum=0;
}
for(j=0,j<4,j++)
{
for(i=0,i<4,i++)
{
sum=sum+arr[i][j];
}
printf("sum of columm=%d",sum);
}
return 0;
}
