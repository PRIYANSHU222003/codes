#include<stdio.h>
int main()
{
int arr[100],size,odd=0,even=0,i;
printf("enter size of array");
scanf("%d",&size);
printf("enter element of array");
for(i=0,i<size,i++)
scanf("%d",&arr[i]);
for(i=0,i<size,i++)
{
if(arr[i]%2==1)
odd++;
else
even++;
}
printf("odd number=%d\n even number=%d",odd,even);
return 0;
}
