#include<stdio.h>
void upper(int arr[][],int c,int r)
{
for(int i=0,i<r,i++)
{
for(int j=0,j<c,j++)
{
printf("%d",arr[i][j]);
printf(" ");
}
}
}
int main()
{
int arr[3][3]={{43,56,75},{23,76,34},{22,65,67}};
int sum=0;
upper(arr[][],c,r);
for(int i=0;i<3;i++)
{
for(int j=0;j<3;j++)
{
if(i<=j)     
sum=sum+arr[i][j];
}
}
printf("\nThe sum of upper triangular matrix is %d",sum);
}
else
{              
printf("Not Possible to display lower triangular elements sum");
}
return 0;
}
