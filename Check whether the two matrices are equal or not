#include<stdio.h>
int main()
{
int r,c;
int i,j;
printf("enter row in  matrics")
scanf("%d",&r);
printf("enter columm in matrics");
scanf("%d",&c);
int a[r][c],b[r][c];
printf("enter element of 1st matrics");
for(i=0,i<r,i++)
{
for(j=0,j<c,j++)
{
printf("[%d][%d]",i,j);
scanf("%d",a[i][j]);
}
}
printf("enter element of 2nd matrics");
for(i=0,i<r,i++)
{
for(j=0,j<c,j++)
{
printf("[%d][%d]",i,j);
scanf("%d",b[i][j]);
}
}
if(a[i][j]=b[i][j])
{
printf("they are equal");
}
else
printf("they are not equal");
return 0;
}
